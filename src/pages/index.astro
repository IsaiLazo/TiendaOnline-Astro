---
import Layout from '../layouts/Layout.astro';
import ProductList from '../components/ProductList.astro';

const products = [
	{ id: 1, name: 'Product 1', price: 19.99, imageUrl: '' },
	{ id: 2, name: 'Product 2', price: 29.99, imageUrl: '' },
	{ id: 3, name: 'Product 3', price: 39.99, imageUrl: '' },
];

---

<Layout>
	<main>
		<h1>Welcome to our Online Store</h1>
		<section>
			<ProductList products={products} />
		</section>
	</main>
</Layout>
